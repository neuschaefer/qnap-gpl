$ rm e/f

$ richacl --set 'owner@:rwx:f:allow root:rx:f:deny root:rx:f:allow' e
$ touch e/f
$ ls -l e/f | cut -d ' ' -f 1
> -rw-------
$ rm e/f

$ richacl --set 'owner@:rwx::allow everyone@:w:fi:deny everyone@:rwx:fi:allow' e
$ touch e/f
$ ls -l e/f | cut -d ' ' -f 1
> -r--r--r--
$ rm e/f

$ richacl --set 'owner@:rwx::allow root:rx:fi:deny root:rx:fi:allow' e
$ touch e/f
$ ls -l e/f | cut -d ' ' -f 1
> ----------
$ rm e/f

$ richacl --set 'owner@:rx:fi:allow group@:rwx:fi:deny everyone@:rwx:f:allow' e
$ touch e/f
$ ls -l e/f | cut -d ' ' -f 1
> -rw----rw-
$ rm e/f

$ richacl --set 'owner@:rx:fi:allow root:rwx:fi:deny everyone@:rwx:f:allow' e
$ touch e/f
