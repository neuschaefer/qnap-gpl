+++ samba-4.7.3/source3/smbd/srvstr.c	2017-11-20 11:56:00.000000000 +0100
@@ -110,6 +110,20 @@ ssize_t message_push_string(uint8_t **ou
 		DEBUG(0, ("srvstr_push failed\n"));
 		return -1;
 	}
+
+	/*
+	 * Ensure we clear out the extra data we have
+	 * grown the buffer by, but not written to.
